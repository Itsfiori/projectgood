<mvc:View
	controllerName="project.goods.controller.overview"
xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
   >


  
    <Table  id="idUsersTable"  items="{/users}">
    <headerToolbar>
    <Toolbar id="idToolbar1" >
    <Title id="idPankajTitle" text="Finished Goods Review"></Title>
 	<SearchField
								id="SearchField1"
								search=".onSearch"
								width="200px"
								liveChange=".onSearchLiveChange"
								suggest=".onSearchSuggest"
								
							/>    
    </Toolbar>
	
    </headerToolbar>

    <columns>
    <Column id="idColumn1" >
    <Text id="idNameText"  text="Invoice Number "/>
    
    </Column>
      <Column id="idColumn" >
    <Text id="_IDGenText2"  text="Invoice Created Date"/>
    
    </Column>
      <Column id="_IDGenColumn3" >
    <Text id="_IDGenText3"  text="Supplier"/>
    
    </Column>
       <Column id="_IDGenColumdn2" >
    <Text id="_IDGendText2"  text="status"/>
    
    </Column>

    
    </columns>
    <ColumnListItem id="_IDGenColumnListItem1" type="Navigation" press="navigateToDeatils">
    <cells>
    <Text id="_IDGenText4"  text="{name}"></Text>
    
    <Text id="_IDGenText5"  text="{email}"></Text>
    
    <Text id="_IDGenText6"  text="{gender}"></Text>
    
    <Text id="_IDGenText7"  text="{status}"></Text>
    
    </cells>
    
    </ColumnListItem>
    
    </Table>
</mvc:View>




