<mvc:View
    controllerName="project.goods.controller.test"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
>
    <App id="onSearchFieldSearch">
        <Page
            id="_IDGenPage1"
            title="Invoice Details" 
        >
            <VBox 
                id="_IDGenVBox1"
                class="sapUiSmallMargin"
            >
                <f:SimpleForm
                    id="onSearchFieldSuggest"
                    editable="true"
                    layout="ResponsiveGridLayout"
                    title="Address"
                    labelSpanL="3"
                    labelSpanM="3"
                    labelSpanS="12"
                    adjustLabelSpan="false"
                    emptySpanXL="4"
                    emptySpanL="4"
                    emptySpanS="0"
                    columnsXL="1"
                    columnsL="1"
                    columnsM="1"
                    singleContainerFullSize="false"
                >
                    <f:content  >
                        <Label
                            id="_IDGenLabel1"
                            text="Name"
                        />
                        <Input
                            id="name"
                            value="{Supplier_Name}"
                        />
                        <Label
                            id="_IDGenLabel2"
                            text="City"
                        />
                        <Input
                            id="Citys"
                            value="City"
                        />

                        <Label
                            id="_IDGenLabel3"
                            text="Country"
                        />
                        <Select
                            id="country"
                            selectedKey="Country"
                        >
                            <items>
                                <core:Item
                                    id="_IDGenItem1"
                                    text="England"
                                    key="England"
                                />
                                <core:Item
                                    id="_IDGenItem2"
                                    text="Germany"
                                    key="Germany"
                                />
                                <core:Item
                                    id="_IDGenItem3"
                                    text="USA"
                                    key="USA"
                                />
                            </items>
                        </Select>

                        <Label
                            id="_IDGenLabel4"
                            text="Tel"
                        />
                        <Input
                            id="Tel"
                            value="Tel"
                        />

                        <Label
                            id="_IDGenLabel5"
                            text="Email"
                        />
                        <Input
                            id="Emails"
                            value="Email"
                        />

                        <Label
                            id="_IDGenLabel6"
                            text="Twitter"
                        />
                        <Input
                            id="TEmail"
                            value="Twitter"
                        />
                    </f:content>
                </f:SimpleForm>
            </VBox>
             <Bar>
    <contentLeft>
   <Text text="@Copyright-ASML"></Text>
   </contentLeft>
    <contentRight>
     <Button id="edit" text="geneerate" press="onGenerateExcelPress" />
     <Button id="save" text="Save" type="Emphasized" visible="true" press="handleSavePress" />
     <Button id="cancel" text="Cancel" visible="true" press="handleCancelPress" />
    </contentRight>
   </Bar>
            <footer>
 

  </footer>
        </Page>
    </App>
</mvc:View>
