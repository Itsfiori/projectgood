    <Table
        id="idUsersTable1"
        width="100%"
    >
        <headerToolbar>
            <Toolbar
                id="_IDGenToolbar1"
                visible="true"
                class="customToolbar"
            >
                <Button
                    text="Submit for Buyer Review"
                    type="Accept"
                />
                <Button
                    text="Upload Template"
                    type="Accept"
                />
                <Button
                    text="Download Template"
                    type="Accept"
                />
            </Toolbar>
        </headerToolbar>

        <columns>
            <Column id="_IDGenColumn1">
                <Text
                    id="_IDGenText1"
                    text="invoice Value"
                />
            </Column>
            <Column id="_IDGenColumn2">
                <Text
                    id="_IDGenText2"
                    text="Curency"
                />
            </Column>
            <Column id="_IDGenColumn3">
                <Text
                    id="_IDGenText3"
                    text="Commodity Code"
                />
            </Column>
            <Column id="_IDGenColumdn2">
                <Text
                    id="_IDGendText2"
                    text="code desc"
                />
            </Column>
        </columns>
        <ColumnListItem
            editable="true"
            id="_IDGenColumnListItem1"
        >
            <cells>
                <Input
                    value="{REF_Invoice_Number}"
                    editable="false"
                />

                <Text
                    id="_IDGenText5"
                    text="a"
                />
                <Text
                    id="_IDGenText6"
                    text="b"
                />
                <Text
                    id="_IDGenText7"
                    text="codedesc"
                />
             
											<ComboBox id="idCountryComboBox" visible="true"
												change="onCountryComboBoxChange"
												placeholder="Compnay Code"
												
											>
												<items>
													<core:Item id="idValue2Item" text="value2"/>
												</items>
											</ComboBox>
            </cells>
        </ColumnListItem>
    </Table>
</mvc:View>
