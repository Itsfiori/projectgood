<mvc:View
    controllerName="project.goods.controller.test"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:u="sap.ui.unified"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns:semantic="sap.f.semantic"
    displayBlock="true"
    height="100%"
    busyIndicatorDelay="0"
>
    <Panel
        id="panelid"
    >
        <semantic:SemanticPage
            editable="true"
            toggleHeaderOnTitleClick="true"
            validationMode="Async"
            editToggled="handleEditToggled"
            showFooter="false"
            labelSpanXL="3"
            labelSpanL="3" 
            labelSpanM="3"
            labelSpanS="12"
        >
			<VBox  items="{/newinvoice}" id="vBox" renderType="Bare">
            <f:SimpleForm 
                id="SimpleFormChange354"
                editable="true"
                layout="ColumnLayout"
                title="New Invoice "
                singleContainerFullSize="true"
            >
                <f:content>
                    <Label
                        id="_IDGenLabel5"
                        text="Supplier Name"
                    />
                    <Input
                        id="_IDGenInput1"
                        value="{Supplier_Name}"
                    />
                    <Label
                        id="_IDGenLabel6"
                        text="Supplier Address"
                    />
                    <Input
                        id="_IDGenInput2"
                        value="{Supplier_Address}"
                    />
                    <Label
                        id="_IDGenLabel7"
                        text="Supplier city"
                    />
                    <Input
                        id="_IDGenInput3"
                        value="{Supplier_City}"
                    />
                    <Label
                        id="_IDGenLabel8"
                        text="Supplier Country"
                    />
                    <Input
                        id="_IDGenInput4"
					value="{Supplier_Country}"
                    />
                    <Label
                        id="_IDGenLabel9"
                        text="Ref Invoice Number"
                    />
                    <Input
                        id="_IDGenInput5"
                        value="{REF_Invoice_Number}"
                    />
                
               
                </f:content>
            </f:SimpleForm>
	</VBox>

            <semantic:positiveAction>
                <semantic:PositiveAction
                    id="_IDGenPositiveAction1"
                    text="Save"
                    press="TriggerSave"
                />
            </semantic:positiveAction>

            <semantic:negativeAction>
                <semantic:NegativeAction
                    id="_IDGenNegativeAction1"
                    text="Delete"
                    visible="{isSaved}"
                    press="TriggerDelete"
                />
            </semantic:negativeAction>
        </semantic:SemanticPage>
    </Panel>
